{
  "name": "ui-kit",
  "version": "0.1.0",
  "description": "A modern, type-safe UI Kit built with Base UI, Vanilla-Extract, and design tokens",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles": {
      "types": "./dist/styles.d.ts",
      "import": "./dist/styles.js",
      "require": "./dist/styles.cjs"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.cjs"
    },
    "./themes": {
      "types": "./dist/themes/index.d.ts",
      "import": "./dist/themes/index.js",
      "require": "./dist/themes/index.cjs"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js",
      "require": "./dist/utils/index.cjs"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "import": "./dist/hooks/index.js",
      "require": "./dist/hooks/index.cjs"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "import": "./dist/constants/index.js",
      "require": "./dist/constants/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "pnpm run clean && pnpm run build:lib && pnpm run build:types",
    "build:lib": "vite build",
    "build:types": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "tokens:build": "style-dictionary build --config ./style-dictionary.config.js"
  },
  "keywords": [
    "ui",
    "ui-kit",
    "component-library",
    "react",
    "typescript",
    "vanilla-extract",
    "base-ui",
    "design-system",
    "theme",
    "accessible"
  ],
  "author": "UI Kit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-kit-team/ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/ui-kit-team/ui-kit/issues"
  },
  "homepage": "https://github.com/ui-kit-team/ui-kit#readme",
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.3",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/css-utils": "^0.1.6",
    "@vanilla-extract/recipes": "^0.5.7",
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.36.0",
    "@storybook/addon-a11y": "^9.1.8",
    "@storybook/addon-docs": "^9.1.8",
    "@storybook/addon-essentials": "9.0.0-alpha.12",
    "@storybook/addon-onboarding": "^9.1.8",
    "@storybook/addon-themes": "^9.1.8",
    "@storybook/addon-vitest": "^9.1.8",
    "@storybook/react-vite": "^9.1.8",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.0.3",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^9.1.8",
    "globals": "^16.4.0",
    "playwright": "^1.55.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "storybook": "^9.1.8",
    "style-dictionary": "^5.0.4",
    "style-dictionary-utils": "^4.1.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.44.0",
    "vite": "npm:rolldown-vite@7.1.12",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.12"
    }
  }
}
