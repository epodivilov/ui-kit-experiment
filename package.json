{
  "name": "ui-kit",
  "version": "0.1.0",
  "description": "A modern, type-safe UI Kit built with Base UI, Vanilla-Extract, and design tokens",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./components/Button": {
      "types": "./dist/components/Button/index.d.ts",
      "default": "./dist/components/Button/index.js"
    },
    "./components/Checkbox": {
      "types": "./dist/components/Checkbox/index.d.ts",
      "default": "./dist/components/Checkbox/index.js"
    },
    "./components/Input": {
      "types": "./dist/components/Input/index.d.ts",
      "default": "./dist/components/Input/index.js"
    },
    "./components/Toast": {
      "types": "./dist/components/Toast/index.d.ts",
      "default": "./dist/components/Toast/index.js"
    },
    "./components/ThemeProvider": {
      "types": "./dist/components/ThemeProvider/index.d.ts",
      "default": "./dist/components/ThemeProvider/index.js"
    },
    "./styles": {
      "default": "./dist/index.css"
    },
    "./themes": {
      "default": "./dist/themes/index.css"
    },
    "./themes/light": {
      "default": "./dist/themes/light.css"
    },
    "./themes/dark": {
      "default": "./dist/themes/dark.css"
    },
    "./dist/*": "./dist/*"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006 --no-open",
    "build": "pnpm build:tokens && pnpm build:components && tsup",
    "build:tokens": "node config/build-tokens.js",
    "build:components": "node config/generate-component-styles.js",
    "clean": "rimraf dist",
    "lint": "eslint . -c config/eslint.config.js",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build"
  },
  "keywords": [
    "ui",
    "ui-kit",
    "component-library",
    "react",
    "typescript",
    "vanilla-extract",
    "base-ui",
    "design-system",
    "theme",
    "accessible"
  ],
  "author": "UI Kit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ui-kit-team/ui-kit.git"
  },
  "bugs": {
    "url": "https://github.com/ui-kit-team/ui-kit/issues"
  },
  "homepage": "https://github.com/ui-kit-team/ui-kit#readme",
  "dependencies": {
    "@base-ui-components/react": "1.0.0-beta.4",
    "@vanilla-extract/css": "^1.17.4",
    "@vanilla-extract/css-utils": "^0.1.6",
    "@vanilla-extract/recipes": "^0.5.7",
    "clsx": "^2.1.1"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.37.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-essentials": "9.0.0-alpha.12",
    "@storybook/addon-onboarding": "^9.1.10",
    "@storybook/addon-themes": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/react": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@types/node": "^24.7.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "@vanilla-extract/esbuild-plugin": "^2.3.18",
    "@vanilla-extract/vite-plugin": "^5.1.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "cross-env": "^10.1.0",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^6.1.1",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-storybook": "^9.1.10",
    "globals": "^16.4.0",
    "playwright": "^1.56.0",
    "prettier": "^3.6.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "rimraf": "^6.0.1",
    "storybook": "^9.1.10",
    "style-dictionary": "^5.1.0",
    "style-dictionary-utils": "^4.1.0",
    "tsup": "^8.5.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.0",
    "vite": "npm:rolldown-vite@7.1.12",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.12"
    }
  }
}
